<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bachelorshelter.anik.medicaladviser.activities.DentistActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/gyn1"
        android:layout_margin="10dp"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Professor Dr. Md. Shamsul Alam\nQualification : BDS, DCD(USSR), FADI(USA)\nChairman,BSM Medical University(Old IPGMR)\nPhone: +880-2-9612345-54, 9615412"
        android:visibility="visible"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/videoButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@color/dot_dark_screen1"
        android:text="video Conference"
        android:textStyle="bold"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/gyn2"
        android:layout_margin="10dp"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="\n\nDr. Salahuddin ( Swapon )\nQualification : B.D.S, FCPS\nAssistant Professor and Head of Departmen BSMMU\nPhone: +880 1711958143"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/videoButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@color/dot_dark_screen1"
        android:text="video Conference"
        android:textStyle="bold"
        tools:ignore="DuplicateIds" />

</LinearLayout>
