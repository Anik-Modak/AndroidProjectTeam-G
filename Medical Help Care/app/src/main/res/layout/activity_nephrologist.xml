<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bachelorshelter.anik.medicaladviser.activities.NephrologistActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/neu1"
        android:layout_margin="10dp"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Professor Dr. Quazi Deen Mohammad
Qualification: MBBS, FCPS, MD (Neurology)
Designation:Director, National Institute of neurosciences"

        android:visibility="visible"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/neu2"
        android:layout_margin="10dp"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Professor Dr. Mansur Habib
Qualification : MBBS, FCPS ( Medicine ) , MD ( Neurology ) MRCP, FRCP
Designation : Professor , Department of Neurology
Expertise : Neuromedicine
Organization: Dhaka Medical College Hospital "
        tools:ignore="MissingConstraints" />

</LinearLayout>
